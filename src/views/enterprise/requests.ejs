<section class="py-5 bg-light">
  <div class="container px-4 px-lg-5 mt-5">
    <h2 class="fw-bolder mb-4">Your minting requests</h2>
    <div class="row gx-4 gx-lg-5 row-cols-2 row-cols-md-3 row-cols-xl-4 justify-content-center">
      <% openMintedRequests.forEach(e=>{ %>
        <div class="col mb-5">
          <div class="card h-100">
              <!-- Product image-->
              <img class="card-img-top" src="https://dummyimage.com/450x300/dee2e6/6c757d.jpg" alt="..." />
              <!-- Product details-->
              <div class="card-body p-4">
                  <div class="text-center">
                      <!-- Enterprise name for Unplastify views -->
                      <% if (userCategory == 3) { %>
                        <h5 class="fw-bolder">
                          <!-- deberia aparecer el nombre de la empresa -->
                          <%= e.user_id %>
                        </h5>
                      <% } %>
                      <!-- Product name-->
                      <h5 class="fw-bolder"><%= e.name %></h5>
                      <!-- Product price-->
                      <span class="text-muted">
                        <%= e.dataValues.minting_status.dataValues.status %>
                      </span>
                  </div>
              </div>
              <!-- Product actions-->
              <div class="card-footer p-4 pt-0 border-top-0 bg-transparent">
                  <div class="text-center"><a class="btn btn-outline-dark mt-auto" href="<%= `/enterprise/mintingRequest/${e.minting_request_id}` %>">Details</a></div>
              </div>
          </div>
        </div>
        <% }) %>
    </div>
  </div>
</section>
<% //if(unplastifiedItem){ %> 
  <%- //include("./modals")%>
<%//} %> 
